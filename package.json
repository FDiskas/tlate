{
  "name": "useful-useless",
  "version": "1.0.0",
  "scripts": {
    "start": "vite",
    "start:dev": "rimraf .cache dist && parcel public/index.html --no-autoinstall --open",
    "start:server": "json-server config/data.json --port 4321 --watch",
    "build": "tsc && vite build",
    "prebuild:start": "yarn build",
    "build:start": "serve -s -o -C -n dist",
    "precommit": "lint-staged",
    "pretest": "npm run lint",
    "test": "jest",
    "lint": "eslint . --cache --fix"
  },
  "lint-staged": {
    "src/**/*.jsx": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.scss": [
      "stylelint --syntax scss",
      "git add"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "bulma": "^0.7.1",
    "classnames": "^2.2.6",
    "font-awesome": "^4.7.0",
    "prop-types": "^15.6.0",
    "react": "^15.0.0 || ^16.0.0 || ^17.0.0",
    "react-dom": "^15.0.0 || ^16.0.0 || ^17.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.0.0",
    "react-router4-with-layouts": "^1.2.8"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.12",
    "@types/jest": "^29.2.3",
    "@types/react": "^15.0.0 || ^16.0.0 || ^17.0.0",
    "@types/react-dom": "^18.0.9",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^2.2.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.6.0",
    "babel-plugin-flow-react-proptypes": "^24.1.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "bun-types": "^0.2.0",
    "concurrently": "^4.0.1",
    "css-module-flow": "^1.0.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-alias": "^1.1.1",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^0.14.3",
    "jest": "^23.6.0",
    "jest-css-modules": "^1.1.0",
    "jest-enzyme": "^6.0.4",
    "json-server": "^0.14.0",
    "lint-staged": "^7.2.0",
    "local-storage-mock": "^1.0.2",
    "module-alias": "^2.1.0",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-imagemin": "^2.1.1",
    "parcel-plugin-json": "^1.0.1",
    "postcss-modules": "^1.1.0",
    "prettier-eslint": "^8.8.2",
    "react-hot-loader": "^4.13.1",
    "rimraf": "^2.6.2",
    "sass": "^1.56.1",
    "serve": "6.5.8",
    "stylelint": "^9.3.0",
    "stylelint-config-recommended": "^2.1.0",
    "vite": "^3.2.4"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "globals": {
      "NODE_ENV": "test"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/config/jest.setup.js"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "type": "module"
}
