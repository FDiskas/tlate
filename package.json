{
  "name": "tlate",
  "version": "0.0.5",
  "private": true,
  "description": "It's never to late - keep all your notes and memory privately in one place",
  "homepage": "https://tlate.com",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "coverage": "react-scripts test --env=jsdom --coverage",
    "toolbox": "react-toolbox-themr"
  },
  "dependencies": {
    "firebase": "^4.1.3",
    "material-design-icons": "^3.0.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-flexbox-grid": "^1.1.3",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^4.0.8",
    "react-style-proptype": "^3.0.0",
    "react-tap-event-plugin": "^2.0.1",
    "react-test-renderer": "^15.6.1",
    "react-toolbox": "2.0.0-beta.6",
    "redux": "^3.7.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "enzyme": "^2.9.1",
    "react-addons-test-utils": "^15.6.0",
    "react-scripts": "1.0.7",
    "react-toolbox-themr": "^1.0.2"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 50,
        "lines": 33,
        "statements": 25
      }
    }
  },
  "reactToolbox": {
    "customProperties": {
      "color-primary-dark": "#607D8B"
    },
    "output": "src/assets/toolbox"
  },
  "engines": {
    "node": ">=6",
    "npm": ">=5"
  }
}
