{
  "name": "tlate",
  "version": "0.0.2",
  "private": true,
  "description": "It's never to late - keep all your notes and memory in privately one place",
  "dependencies": {
    "firebase": "^3.6.10",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-tap-event-plugin": "^2.0.1",
    "react-toolbox": "^2.0.0-beta.7"
  },
  "devDependencies": {
    "enzyme": "^2.7.1",
    "react-addons-test-utils": "^15.4.2",
    "react-scripts": "0.9.3",
    "react-test-renderer": "^15.4.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "coverage": "react-scripts test --env=jsdom --coverage"
  },
  "jest": {
    "colectCoverageFrom": [
      "src/*.js"
